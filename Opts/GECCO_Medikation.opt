        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Das Datum / die Uhrzeit der letzten Verschreibung des Arzneimittels.</items>
          <items id="text">Zuletzt verschrieben</items>
        </term_definitions>
        <term_definitions code="at0026">
